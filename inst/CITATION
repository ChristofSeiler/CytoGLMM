citHeader("To cite the CytoGLMM package in publications, use:")

citEntry(
  entry = "article",
  author = personList(person(given = "Christof", family = "Seiler"),
    person(given = "Lisa", family = "Kronstad", middle = "M"),
    person(given = "Laura", family = "Simpson", middle = "J"),
    person(given = "Mathieu", family = "Le Gars"),
    person(given = "Elena", family = "Vendrame"),
    person(given = "Catherine", family = "Blish", middle = "A"),
    person(given = "Susan", family = "Holmes")),
  title = "Uncertainty Quantification in Multivariate Mixed Models for Mass Cytometry Data",
  journal = "arXiv",
  year = 2019,
  textVersion = paste(
    "Christof Seiler, Lisa M. Kronstad, Laura J. Simpson, Mathieu Le Gars, Elena Vendrame, Catherine A. Blish, and Susan Holmes,",
    "Uncertainty Quantification in Multivariate Mixed Models for Mass Cytometry Data,",
    "arXiv, 2019"
  )
)
